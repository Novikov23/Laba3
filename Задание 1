<body>
    <?php
        $name = array ("Витя", "Люся");
        foreach ($name as $key => $value){
             if ($value == "Витя")
        echo $key."<br />";
            
            else unset ($name [$key]) ;
            
        }
        foreach ($name as $key => $value){
             echo $value."<br />";
         }
        foreach ($name as $key => $value)
        {
            if ($value == "Витя")
            unset ($value);
            echo $name;
        }
       
 ?>
</body>
